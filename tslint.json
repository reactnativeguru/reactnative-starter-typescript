
{
    "extends": [
      "tslint:recommended",
      "tslint-eslint-rules",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-empty-interface": false,
        "member-access": [true, "no-public"],
        "ordered-imports": [false],
        "object-literal-sort-keys": [false],
      "interface-name": false,
      "jsx-no-lambda": false,
      "object-literal-sort-keys": false,
      "quotemark": [true, "single", "jsx-double"]
    }
  }